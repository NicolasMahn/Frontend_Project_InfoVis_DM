{
  "manifest_version": 2,
  "short_name": "Dashboard",
  "name": "GASTech Employee Investigation Dashboard",
  "version": "1.0",
  "description": "Your extension description.",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    },
    {
      "src": "gastechlogo_192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "gastechlogo_512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "web_accessible_resources": [
    "graphs.yml"
  ]
}
